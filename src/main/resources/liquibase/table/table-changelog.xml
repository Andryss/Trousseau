<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <include file="roles.sql" relativeToChangelogFile="true"/>
    <include file="user_roles.sql" relativeToChangelogFile="true"/>
    <include file="privileges.sql" relativeToChangelogFile="true"/>
    <include file="role_privileges.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>
